package com.classes;


public class Carros {

	private int id;
	private String modelo;
	private String marca;

	public Carros() {
		super();
	}

	public Carros(int id, String moledo, String marca) {
		super();
		this.id = id;
		this.modelo = moledo;
		this.marca = marca;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getMoledo() {
		return modelo;
	}

	public void setMoledo(String moledo) {
		this.modelo = moledo;
	}

	public String getMarca() {
		return marca;
	}

	public void setMarca(String marca) {
		this.marca = marca;
	}

}
